{
  "expo": {
    "name": "MarkMe",
    "slug": "markme",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.xendev.markme",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      "expo-font",
      [
        "@react-native-community/datetimepicker",
        {
          "android": {
            "datePicker": {
              "colorAccent": {
                "light": "#1e1e1e"
              },
              "textColorPrimary": {
                "light": "#ffe93f"
              }
            },
            "timePicker": {
              "background": {
                "light": "#1e1e1e",
                "dark": "#1e1e1e"
              },
              "numbersBackgroundColor": {
                "light": "#1e1e1e",
                "dark": "#1e1e1e"
              }
            }
          }
        }
      ],
      [
        "expo-asset",
        {
          "assets":[
            "./assets/images/event.png"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b478630c-e0c3-4cda-8df1-861f12662480"
      }
    }
  }
}
